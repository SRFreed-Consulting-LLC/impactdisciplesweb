{
  "name": "impactdisciplesweb",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "build-dev": "ng build --configuration development",
    "build-prod": "ng build --configuration production",
    "build-deploy": "ng build --configuration production && firebase deploy --only hosting:impactdisciples-public",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "firebase deploy --only hosting:impactdisciples-public",
    "add-git-submodule": "git submodule add -b development https://github.com/SRFreed-Consulting-LLC/impactdisciplescommon.git",
    "init-git-submodules": "git submodule update --init --recursive && git submodule status --recursive"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/fire": "^16.0.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@google-cloud/firestore": "^7.9.0",
    "bootstrap": "^5.1.3",
    "date-fns": "^3.6.0",
    "devextreme": "23.2.6",
    "devextreme-angular": "23.2.6",
    "devextreme-schematics": "^1.6.9",
    "firebase": "^10.12.2",
    "ngx-cookie-service": "^16.1.0",
    "ngx-slider-v2": "^16.0.0",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "stripe": "^16.7.0",
    "swiper": "^11.1.9",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.14",
    "@angular/cli": "^16.2.14",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "devextreme-cli": "latest",
    "devextreme-themebuilder": "23.2.6",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.3"
  }
}
